<h1>Donors!</h1>


{{#each donors}}
    <h2>
        <a href="/donors/{{this._id}}">
            {{this.name}}
        </a>
    </h2>
    {{#if this.status}}
        <h1>approved</h1>
    <!-- <small>{{this.age}}</small>
    <small>{{this.race}}</small>
    <small>{{this.college}}</small>
    <small>{{this.approved}}</small> -->
    {{else}}
        <p> review </p>
        {{log this.status}}
    {{/if}}

{{/each}}
